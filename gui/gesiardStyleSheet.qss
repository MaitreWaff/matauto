    font: 11px;
}

QLabel#msgLogoLabel {
    font: bold 28px;
}

QLabel#labelSinistreEnCours {
    background-color: mediumorchid;
    color: white;
    font: bold 23px;
    padding: 5px 1px;
    margin-top: 3px;
    margin-bottom: 3px;
}

QLabel#labelVictimesEnregistrees {
    background-color: darkmagenta;
    color: white;
    font: bold 16pt;
}

QLabel, QLineEdit, QComboBox {
    border-color: black;
    border-width: 0px;
    padding: 0 -1px;
    margin: 1px;
}

QLabel {
    background-color: oldlace;
    margin-right: 0px;
    margin-left: 0px;
    margin-top: -2px;
    margin-bottom: -2px;
    padding: 0px 0px 0px 1px;
}

QLabel#labelEmissions, QLabel#labelPaiements, QLabel#labelSolde, QLabel#labelDebits, QLabel#labelRistournes,
QLabel#labelComptant, QLabel#labelEmission, QLabel#labelPaiement {
    font: bold 11px;
    background-color: springgreen;
    padding: 5px 0px;
    margin-top: 3px;
    margin-bottom: 3px;
}

QLabel#labelInfoSurTiers {
    padding: 5px 0px;
    margin-top: 3px;
    margin-bottom: 3px;
}

QLabel#labelDetailsGaranties, QLabel#labelRecapPrimes, QLabel#labelMouvAssocies, QLabel#labelTypeDoc {
    font: bold 14px;
    background-color: none;
    padding: 3px 0px;
}

QLineEdit#emissionLineEdit, QLineEdit#emissionsLineEdit, QLineEdit#paiementLineEdit, QLineEdit#paiementsLineEdit,
QLineEdit#soldeLineEdit, QLineEdit#debitsLineEdit, QLineEdit#ristourneLineEdit, QLineEdit#comptantLineEdit {
    background-color: lime;
}

QLineEdit {
    background-color: white;
    margin-left: 0px;
}

QTableView {
    border: solid 1px;
}

QTextBrowser#perGarantCouvrSinistrTextBrowser {
