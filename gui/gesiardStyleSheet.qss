}

QLabel#labelAnalysePrejudices {
    background-color: navy;
    color: white;
    font: bold 18px;
}

QLabel#labelAssureurVictimesSinistre {
    background-color: olive;
    color: white;
    font: bold 18px;
}

QLabel#labelEtatCivilVictime {
    font: bold 18px;
}

QLabel#labelInfoSurTiers {
    font: 15px;
}

